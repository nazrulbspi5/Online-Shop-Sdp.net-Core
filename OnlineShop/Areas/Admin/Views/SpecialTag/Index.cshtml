@model IEnumerable<OnlineShop.Models.SpecialTag>
    @{
    ViewData["Title"] = "Index";
    }

    <br /><br />
    <div class="row">
        <div class="col-md-6">
            <h2 class="text-info">Tag Name List</h2>
        </div>
        <div class="col-md-6 text-right">
            <a asp-action="Create" class="btn btn-info"><i class="fas fa-plus"></i>&nbsp;New Product Type</a>
        </div>
    </div>
    <div class="row">
        <table class="table table-striped table-bordered">
            <tr class="table-info">
                <th>@Html.DisplayNameFor(c=>c.TagName)</th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.TagName</td>
                <td>
                    <partial name="_ButtonPartial" model="@item.Id" />
                </td>
            </tr>
        }
        </table>
    </div>

